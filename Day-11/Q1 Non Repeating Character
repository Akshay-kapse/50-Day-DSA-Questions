// ğŸ§‘â€ğŸ’» Language: JavaScript

/**
 * @param {string} s
 * @returns {string}
 */
class Solution {
    nonRepeatingChar(s) {
        let uique = [...s].filter((item, _, string) => {
            return string.indexOf(item) === string.lastIndexOf(item)
        })

        if (uique.length >= 1) return uique[0]
        return '$'
    }
}
