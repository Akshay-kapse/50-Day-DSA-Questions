var topKFrequent = function (nums, k) {
    let map = new Map()
    for (let num of nums) {
        map.set(num, (map.get(num) || 0) + 1)
    }

    let sorted = [...map.entries()].sort((a, b) => b[1] - a[1])
    return sorted.slice(0, k).map(item => item[0])
};
