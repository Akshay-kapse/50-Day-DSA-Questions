/**
 * @param {string[]} strs
 * @return {string[][]}
 */
var groupAnagrams = function(strs) {
    let map = new Map()

    for (let str of strs) {
        let Ar = str.split("").sort().join("")
        if (!map.has(Ar)) {
            map.set(Ar, [])
        }
        map.get(Ar).push(str)
    }
    return Array.from(map.values())
};
