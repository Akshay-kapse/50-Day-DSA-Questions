/**
 * @param {number} n
 * @return {number}
 */
var numTrees = function (n) {
    let res = 1
    let n1 = n + 1

    for (let i = 2; i <= 2 * n; i++) {
        if (i <= n) n1 *= i
        else {
            res *= i
        }
    }
    return Math.round(res / n1)
};
