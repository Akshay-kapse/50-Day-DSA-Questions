/**
 * @param {number[]} nums
 * @return {number}
 */
var jump = function (nums) {
    let fasted = 0
    let currentEnd = 0
    let jump = 0

    for (let i = 0; i < nums.length - 1; i++) {
        fasted = Math.max(fasted, i + nums[i])

        if (i === currentEnd) {
            jump++
            currentEnd = fasted
        }
    }
    return jump
};
