// 🔸 Problem: 1317. Convert Integer to the Sum of Two No-Zero Integers
// 🔹 Difficulty: Easy
// 🔗 Link: https://leetcode.com/problems/convert-integer-to-the-sum-of-two-no-zero-integers/
// 🧠 Solved by: Akshay Kapse | 50-Day DSA Challenge (Day 15)
// 📅 Date: 2025-09-08
// 🧑‍💻 Language: JavaScript

/**
 * @param {number} n
 * @return {number[]}
 */
var getNoZeroIntegers = function (n) {
    for (let i = 1; i < n; i++) {
        let A = i;
        let B = n - i;

        if ((A + B) === n && !A.toString().includes('0') && !B.toString().includes('0')) {
            return [A, B];
        }
    }
    return [];
};
